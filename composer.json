{
    "name": "root/boosterthon_project",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AppBundle\\": "src/AppBundle",
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4",
        "symfony/console": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/flex": "^2",
        "symfony/form": "^6.4",
        "symfony/runtime": "^6.4",
        "symfony/security-bundle": "^6.4",
        "symfony/twig-bundle": "^6.4",
        "symfony/validator": "^6.4",
        "symfony/yaml": "^6.4",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/orm": "^2.17",
        "symfony/mailer": "^6.4",
        "symfony/monolog-bundle": "^3.10",
        "twig/twig": "^3.0",
        "symfony/asset": "^6.4"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.4",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    },
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    }
}
